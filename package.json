{
  "name": "knowelege-base",
  "version": "0.0.1",
  "description": "Backend",
  "private": true,
  "scripts": {
    "watch": "tsc-watch --onSuccess \"ts-node ./src/index.ts\"",
    "lint": "eslint \"**/*.ts\"",
    "db:init": "initdb -D ${CLUSTER_NAME:-pg} -E UTF8 --locale=en_US.UTF-8",
    "db:start": "postgres -D ${CLUSTER_NAME:-pg} -p 5300",
    "prisma:migrate-dev": "prisma migrate dev --name",
    "prisma:generate": "prisma generate"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.3.3",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.2.0",
    "typescript": "5.4.2"
  },
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/jwt": "^8.0.1",
    "@prisma/client": "^5.21.0",
    "@types/pg": "^8.11.10",
    "dotenv-flow": "^4.1.0",
    "fastify": "^4.26.2",
    "fastify-extract-definitions": "^3.1.0",
    "fastify-plugin": "^5.0.1",
    "prisma": "^5.21.0"
  },
  "main": "./src"
}
